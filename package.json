  },
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "core-js": "^2.5.4",
    "dom-testing-library": "^3.11.1",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.7.1",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@types/node": "~10.5.4",
    "codelyzer": "~4.4.2",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "jest-preset-angular": "^6.0.1",
    "lint-staged": "^7.2.0",
    "ng-packagr": "^3.0.6",
    "prettier": "^1.13.7",
    "semantic-release": "^15.8.1",
    "ts-node": "~7.0.0",
    "tsickle": "0.32.1",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3"
  }
